{
  "name": "vue-i18n-chinese-extractor",
  "version": "0.0.1",
  "description": "scan .vue, export Chinese Text Json and set $t() by Chinese key",
  "main": "./src/lib/index.ts",
  "scripts": {
    "test": "ts-node ./src/bin/index.ts tests/",
    "build": "tsc",
    "start": "npm run build",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "version": "git add -A .",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmislm/vue-i18n-getChinese.git"
  },
  "keywords": [
    "vue-i18n",
    "tool",
    "helper"
  ],
  "author": "lmislm",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lmislm/vue-i18n-getChinese/issues"
  },
  "homepage": "https://github.com/lmislm/vue-i18n-getChinese#readme",
  "devDependencies": {
    "@babel/parser": "^7.4.3",
    "@babel/types": "^7.4.0",
    "@types/glob": "^7.1.1",
    "@types/node": "^11.13.5",
    "@types/rimraf": "^2.0.2",
    "glob": "^7.1.3",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.4"
  },
  "dependencies": {
    "commander": "^2.20.0"
  }
}
